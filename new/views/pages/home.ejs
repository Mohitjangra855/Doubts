
<body>
    <% layout("/boilerplates/boilerplate") %>
       
        <div class="row row-cols-lg-3 row-cols-md-2 row-cols-sm-1">
            <% for(let d of data){ %>
                <a href="/<%= d._id  %>" class="listing-link">
                    <div class="card col mb-2">

                        <img src="<%= d.image %>" class="card-img-top" alt="listing_image" style="height: 20rem;">
                        <div class="card-img-overlay"></div>
                        <div class="card-body">
                            <p class="card-text">
                                <b>
                                    <%= d.title %>
                                </b><br>
                               
                            </p>
                        </div>

                    </div>
                </a>
                <% } %>
        </div>

</body>

